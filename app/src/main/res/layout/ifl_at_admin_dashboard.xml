<?xml version="1.0" encoding="utf-8"?>
<instafel.app.ui.Page
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="instafel.app.instafel.app.activity.about.ifl_a_about">

    <instafel.app.ui.PageTitle
        android:id="@+id/ifl_page_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:ifl_attr_ui_titleText="Admin Dashboard" />

    <instafel.app.ui.PageContentArea
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ifl_page_title">

        <instafel.app.ui.TileTitle
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:ifl_attr_ui_titleText="Actions" />

        <instafel.app.ui.TileCompact
            android:id="@+id/ifl_tile_admin_update_approve_preview"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:ifl_attr_ui_iconRes="@drawable/ifl_action"
            app:ifl_attr_ui_spaceBottom="visible"
            app:ifl_attr_ui_titleText="Approve Preview" />

        <instafel.app.ui.TileCompact
            android:id="@+id/ifl_tile_admin_update_update_backup"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:ifl_attr_ui_iconRes="@drawable/ifl_action"
            app:ifl_attr_ui_titleText="Update Backup" />

        <instafel.app.ui.TileTitle
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:ifl_attr_ui_titleText="Other Actions" />

        <instafel.app.ui.TileCompact
            android:id="@+id/ifl_tile_admin_action_sharedpref_manager"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:ifl_attr_ui_iconRes="@drawable/ifl_action"
            app:ifl_attr_ui_spaceBottom="visible"
            app:ifl_attr_ui_titleText="Preference Manager" />

        <instafel.app.ui.TileCompact
            android:id="@+id/ifl_tile_admin_action_export_mapping"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:ifl_attr_ui_iconRes="@drawable/ifl_action"
            app:ifl_attr_ui_spaceBottom="visible"
            app:ifl_attr_ui_titleText="Export Mapping File" />

        <instafel.app.ui.TileCompact
            android:id="@+id/ifl_tile_admin_action_logout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:ifl_attr_ui_iconRes="@drawable/ifl_action"
            app:ifl_attr_ui_spaceBottom="visible"
            app:ifl_attr_ui_titleText="Log Out" />

    </instafel.app.ui.PageContentArea>

</instafel.app.ui.Page>
