<?xml version="1.0" encoding="utf-8"?>
<instafel.app.ui.Page
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="instafel.app.instafel.app.activity.crash_manager.ifl_a_crash_viewer">

    <instafel.app.ui.PageTitle
        android:id="@+id/ifl_page_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:ifl_attr_ui_titleText="@string/ifl_a8_01" />

    <instafel.app.ui.PageContentArea
        android:id="@+id/ifl_page_area"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ifl_page_title">

        <instafel.app.ui.TileCompact
            android:id="@+id/ifl_tile_crashviewer_copy"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:ifl_attr_ui_iconRes="@drawable/ifl_copy"
            app:ifl_attr_ui_subtitleText="None"
            app:ifl_attr_ui_titleText="@string/ifl_a8_02" />

        <instafel.app.ui.TileCompact
            android:id="@+id/ifl_tile_crashviewer_share"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:ifl_attr_ui_spaceTop="visible"
            app:ifl_attr_ui_iconRes="@drawable/ifl_share"
            app:ifl_attr_ui_subtitleText="None"
            app:ifl_attr_ui_titleText="@string/ifl_a8_03" />

        <instafel.app.ui.TileTitle
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:ifl_attr_ui_titleText="@string/ifl_a8_04"/>

        <instafel.app.ui.TileLarge
            android:id="@+id/ifl_tile_crashviewer_aver"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:ifl_attr_ui_enableIcon="gone"
            app:ifl_attr_ui_spaceBottom="visible"
            app:ifl_attr_ui_subtitleText="None"
            app:ifl_attr_ui_titleText="@string/ifl_a8_05" />

        <instafel.app.ui.TileLarge
            android:id="@+id/ifl_tile_crashviewer_iflIgVersion"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:ifl_attr_ui_enableIcon="gone"
            app:ifl_attr_ui_spaceBottom="visible"
            app:ifl_attr_ui_subtitleText="None"
            app:ifl_attr_ui_titleText="@string/ifl_a8_06" />

        <instafel.app.ui.TileLarge
            android:id="@+id/ifl_tile_crashviewer_crashDate"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:ifl_attr_ui_enableIcon="gone"
            app:ifl_attr_ui_subtitleText="None"
            app:ifl_attr_ui_titleText="@string/ifl_a8_07" />

        <instafel.app.ui.TileTitle
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:ifl_attr_ui_titleText="@string/ifl_a8_08"/>

        <instafel.app.ui.TileLarge
            android:id="@+id/ifl_tile_crashviewer_msg"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:ifl_attr_ui_spaceBottom="visible"
            app:ifl_attr_ui_enableSubIcon="visible"
            app:ifl_attr_ui_subIconRes="@drawable/ifl_copy"
            app:ifl_attr_ui_subtitleText="None"
            app:ifl_attr_ui_enableIcon="gone"
            app:ifl_attr_ui_titleText="@string/ifl_a8_09" />

        <instafel.app.ui.TileLarge
            android:id="@+id/ifl_tile_crashviewer_stacktrace"
            app:ifl_attr_ui_enableSubIcon="visible"
            app:ifl_attr_ui_subIconRes="@drawable/ifl_copy"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:ifl_attr_ui_enableIcon="gone"
            app:ifl_attr_ui_subtitleText="None"
            app:ifl_attr_ui_titleText="@string/ifl_a8_10" />
    </instafel.app.ui.PageContentArea>

</instafel.app.ui.Page>