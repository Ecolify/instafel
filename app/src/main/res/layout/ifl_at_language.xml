<?xml version="1.0" encoding="utf-8"?>
<me.mamiiblt.instafel.ui.Page
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="me.mamiiblt.instafel.activity.ifl_a_language">

    <me.mamiiblt.instafel.ui.PageTitle
        android:id="@+id/ifl_page_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:ifl_attr_ui_titleText="@string/ifl_a0_03" />

    <me.mamiiblt.instafel.ui.PageContentArea
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ifl_page_title">

        <me.mamiiblt.instafel.ui.TileLargeSwitch
            android:id="@+id/ifl_tile_lang_device"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:ifl_attr_ui_iconRes="@drawable/ifl_device"
            app:ifl_attr_ui_spaceBottom="visible"
            app:ifl_attr_ui_subtitleText="_device_language"
            app:ifl_attr_ui_titleText="@string/ifl_a6_01" />

        <me.mamiiblt.instafel.ui.TileLarge
            android:id="@+id/ifl_tile_lang_english"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:ifl_attr_ui_iconResTint="false"
            app:ifl_attr_ui_iconRes="@drawable/ifl_flag_us"
            app:ifl_attr_ui_spaceBottom="visible"
            app:ifl_attr_ui_enableSubIcon="gone"
            app:ifl_attr_ui_subIconRes="@drawable/ifl_tick"
            app:ifl_attr_ui_subtitleText="United States"
            app:ifl_attr_ui_titleText="English" />

        <me.mamiiblt.instafel.ui.TileLarge
            android:id="@+id/ifl_tile_lang_turkish"
            android:layout_width="match_parent"
