        <instafel.app.ui.TileCompact android:id="@+id/ifl_tile_menu_crashlogs" android:visibility="visible" android:layout_width="fill_parent" android:layout_height="wrap_content" app:ifl_attr_ui_iconPadding="2" app:ifl_attr_ui_iconRes="@drawable/ifl_crashlog" app:ifl_attr_ui_spaceBottom="visible" app:ifl_attr_ui_titleText="@string/ifl_a0_07" />
        <instafel.app.ui.TileCompact android:id="@+id/ifl_tile_menu_ghost_mode" android:visibility="visible" android:layout_width="fill_parent" android:layout_height="wrap_content" app:ifl_attr_ui_iconPadding="2" app:ifl_attr_ui_iconRes="@drawable/ifl_misc" app:ifl_attr_ui_spaceBottom="visible" app:ifl_attr_ui_titleText="Ghost Mode" />
